# Google Analytics
NEXT_PUBLIC_GA_ID=your-google-analytics-id

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://helluniversity.com

# Development
NODE_ENV=development

# Upstash Redis (Rate Limiting)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
RATE_LIMIT=2
RATE_WINDOW=10 m

# Provider API (Decorate Backend)
# Point to local SDXL service or remote API endpoint
# Local dev default:
HF_API_URL=http://localhost:8000/v1/decorate
# Must match SDXL service API_AUTH_TOKEN
HF_API_TOKEN=your-strong-shared-token

# SDXL generation defaults
HF_USE_CONTROLNET_DEPTH=false
HF_STEPS=28
HF_GUIDANCE_SCALE=5
HF_DENOISE_STRENGTH=0.3