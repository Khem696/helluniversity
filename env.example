# Google Analytics
NEXT_PUBLIC_GA_ID=your-google-analytics-id

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://helluniversity.com

# Development
NODE_ENV=development

# Upstash Redis (Rate Limiting)
UPSTASH_REDIS_REST_URL=
